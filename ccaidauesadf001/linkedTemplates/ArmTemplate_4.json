{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ccaidauesadf001"
		},
		"eTax_properties_eTax 1 File Management_parameters_directory": {
			"type": "string",
			"defaultValue": "VAT Output 2\\VAT Output 3"
		},
		"eTax_properties_eTax 1 File Management_parameters_secretCore": {
			"type": "string",
			"defaultValue": "azure-sql-prd-core"
		},
		"eTax_properties_eTax 1 File Management_parameters_secretSales": {
			"type": "string",
			"defaultValue": "azure-sql-prd-sales"
		},
		"eTax_properties_eTax 1 File Management_parameters_maxFileProcessed": {
			"type": "int",
			"defaultValue": 100
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/eTax')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "eTax 1 File Management",
							"type": "PipelineReference"
						},
						"parameters": {
							"directory": "[parameters('eTax_properties_eTax 1 File Management_parameters_directory')]",
							"secretCore": "[parameters('eTax_properties_eTax 1 File Management_parameters_secretCore')]",
							"secretSales": "[parameters('eTax_properties_eTax 1 File Management_parameters_secretSales')]",
							"maxFileProcessed": "[parameters('eTax_properties_eTax 1 File Management_parameters_maxFileProcessed')]"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 10,
						"startTime": "2019-12-10T05:16:00Z",
						"timeZone": "UTC"
					}
				}
			},
			"dependsOn": []
		}
	]
}