{
	"name": "Integration Data",
	"properties": {
		"activities": [
			{
				"name": "Copy data",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "List All Data",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": false,
							"wildcardFolderPath": "integration/zipped",
							"wildcardFileName": "SendUserGps.zip",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "\"PinID\"|\"EmployeeName\"|\"PlotTime\"|\"Longitude\"|\"Latitude\"|\"EmployeeName2\"",
									"type": "String"
								},
								"sink": {
									"name": "\"PinID\"|\"EmployeeName\"|\"PlotTime\"|\"Longitude\"|\"Latitude\"|\"EmployeeName2\"",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "UnprocessedCSV",
						"type": "DatasetReference",
						"parameters": {
							"deliminator": {
								"value": "@pipeline().parameters.deliminator",
								"type": "Expression"
							},
							"compressionType": {
								"value": "@if(pipeline().parameters.isZipped,variables('compressionType'),'none')",
								"type": "Expression"
							},
							"compressionLevel": {
								"value": "@if(pipeline().parameters.isZipped,variables('compressionLevel'),'none')",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "Production_Report",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "List All Data",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource"
					},
					"dataset": {
						"referenceName": "Production_Report",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			}
		],
		"parameters": {
			"deliminator": {
				"type": "string",
				"defaultValue": ","
			},
			"isZipped": {
				"type": "bool",
				"defaultValue": false
			}
		},
		"variables": {
			"spInsertTable": {
				"type": "String"
			},
			"typeTable": {
				"type": "String"
			},
			"tableTableParameter": {
				"type": "String"
			},
			"compressionType": {
				"type": "String",
				"defaultValue": "ZipDeflate"
			},
			"compressionLevel": {
				"type": "String",
				"defaultValue": "Optimal"
			}
		},
		"folder": {
			"name": "Production"
		},
		"annotations": []
	}
}