{
	"name": "Copy Blob To SQL",
	"properties": {
		"activities": [
			{
				"name": "Copy blob to SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"sqlWriterStoredProcedureName": {
							"value": "spStagging_DMSUserGps_Insert",
							"type": "Expression"
						},
						"sqlWriterTableType": {
							"value": "typeStagging_DMSUserGps",
							"type": "Expression"
						},
						"storedProcedureTableTypeParameterName": {
							"value": "typeStagging_DMSUserGps",
							"type": "Expression"
						},
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "PinID"
								},
								"sink": {
									"name": "PinID"
								}
							},
							{
								"source": {
									"name": "EmployeeName",
									"type": "String"
								},
								"sink": {
									"name": "EmployeeName"
								}
							},
							{
								"source": {
									"name": "PlotTime",
									"type": "DateTime"
								},
								"sink": {
									"name": "PlotTime"
								}
							},
							{
								"source": {
									"name": "Longitude",
									"type": "Decimal"
								},
								"sink": {
									"name": "Longitude"
								}
							},
							{
								"source": {
									"name": "Latitude",
									"type": "Decimal"
								},
								"sink": {
									"name": "Latitude"
								}
							},
							{
								"source": {
									"name": "EmployeeName2"
								},
								"sink": {
									"name": "EmployeeName2"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "ZipFile",
						"type": "DatasetReference",
						"parameters": {
							"container": {
								"value": "@pipeline().parameters.container",
								"type": "Expression"
							},
							"directory": {
								"value": "@pipeline().parameters.directory",
								"type": "Expression"
							},
							"filename": {
								"value": "@pipeline().parameters.filename",
								"type": "Expression"
							},
							"deliminator": {
								"value": "@pipeline().parameters.deliminator",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "Production_Report",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"container": {
				"type": "string"
			},
			"directory": {
				"type": "string"
			},
			"filename": {
				"type": "string"
			},
			"deliminator": {
				"type": "string"
			},
			"isZipped": {
				"type": "bool"
			}
		},
		"variables": {
			"spInsertTable": {
				"type": "String"
			},
			"typeTable": {
				"type": "String"
			},
			"tableTableParameter": {
				"type": "String"
			},
			"compressionType": {
				"type": "String",
				"defaultValue": "ZipDeflate"
			},
			"compressionLevel": {
				"type": "String",
				"defaultValue": "Optimal"
			},
			"mapping": {
				"type": "String"
			}
		},
		"folder": {
			"name": "Master"
		},
		"annotations": []
	}
}